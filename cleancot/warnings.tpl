<!-- BEGIN: ERROR -->

    <div class="row">
        <div class="col-12">
            <div class="alert alert-danger mt-4 mb-4" role="alert">
                <h4 class="alert-heading h6">{PHP.L.Error}</h4>
                <ul class="mb-0">
                    <!-- BEGIN: ERROR_ROW -->
                    <li>{ERROR_ROW_MSG}</li>
                    <!-- END: ERROR_ROW -->
                </ul>
            </div>
        </div>
    </div>

<!-- END: ERROR -->

<!-- BEGIN: WARNING -->

    <div class="row">
        <div class="col-12">
            <div class="alert alert-warning mt-4 mb-4" role="alert">
                <h4 class="alert-heading h6">{PHP.L.Warning}</h4>
                <ul class="mb-0">
                    <!-- BEGIN: WARNING_ROW -->
                    <li>{WARNING_ROW_MSG}</li>
                    <!-- END: WARNING_ROW -->
                </ul>
            </div>
        </div>
    </div>

<!-- END: WARNING -->

<!-- BEGIN: DONE -->

    <div class="row">
        <div class="col-12">
            <div class="alert alert-success mt-4 mb-4" role="alert">
                <h4 class="alert-heading h6">{PHP.L.Done}</h4>
                <ul class="mb-0">
                    <!-- BEGIN: DONE_ROW -->
                    <li>{DONE_ROW_MSG}</li>
                    <!-- END: DONE_ROW -->
                </ul>
            </div>
        </div>
    </div>

<!-- END: DONE -->
